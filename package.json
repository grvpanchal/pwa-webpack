{
  "name": "pwa-webpack",
  "version": "1.0.0",
  "description": "A PWA using webpack",
  "scripts": {
    "start": "npm run dev",
    "start:pwa": "npm run dev:pwa",
    "build": "webpack -p --mode production",
    "build:dev": "webpack -d --mode development",
    "dev": "webpack-dev-server --http2 --progress",
    "dev:pwa": "concurrently 'node http-server.js' 'webpack-dev-server --progress --port 443'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack",
    "pwa"
  ],
  "author": "Gaurav Panchal",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {}
}
